{
  "name": "node-sds",
  "version": "1.3.0",
  "description": "Node.js module to communicate with JANUS-based apps",
  "repository": {
    "type": "git",
    "url": "https://github.com/otris/node-sds"
  },
  "license": "MIT",
  "main": "out/src/main.js",
  "types": "node-sds.d.ts",
  "publisher": "otris-software",
  "author": "otris software AG",
  "contributors": [
    "Maximilian Wehrstedt <wehrstedt@otris.de>"
  ],
  "scripts": {
    "beautify": "npm run lint -- --fix",
    "compile": "tsc -p ./",
    "doc": "typedoc --out doc src",
    "generate-package-lock": "node -e \"var fs = require('fs'); if (fs.existsSync('package-lock.json')) fs.unlinkSync('package-lock.json')\" && synp --source-file yarn.lock",
    "generate-todo": "leasot -x --reporter markdown src/**/*.ts test/**/*.ts > TODO.md",
    "lint": "tslint -c ./tslint.json -p test/tsconfig.json",
    "precommit": "ts-node scripts/precommit.ts",
    "prepublish": "npm run generate-package-lock",
    "test": "tsc -p ./test && mocha --timeout 20000 -u tdd --recursive out/test/"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^2.2.38",
    "@types/node": "^7.0.4",
    "@types/promise.prototype.finally": "^2.0.2",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "husky": "^0.14.3",
    "mocha": "^2.4.5",
    "shelljs": "^0.8.2",
    "synp": "^1.3.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.3.3"
  },
  "dependencies": {
    "leasot": "^6.3.0",
    "promise.prototype.finally": "^3.1.0",
    "promised-timeout": "^0.3.3"
  }
}
